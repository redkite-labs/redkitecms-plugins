{% extends base_template %}
{% block metatags %}
    {{ parent() }}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
{% endblock %}

{% block post_external_stylesheets %}
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
    <link href='/plugins/agencytheme/css/agency.css' rel='stylesheet' type='text/css'>
    {{ parent() }}
{% endblock %}


{% block external_stylesheets %}
    {% for value in template_assets_manager.getExternalStylesheets(['cssrewrite']) %}
        <link href="{{ value }}" rel="stylesheet" type="text/css" media="all" />
    {% endfor %}
{% endblock %}

{% block external_javascripts %}
    {% for value in template_assets_manager.getExternalJavascripts %}
        <script src="{{ value }}"></script>
    {% endfor %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
{% endblock %}

{% block internal_body_javascripts %}
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <script src="/plugins/agencytheme/js/classie.js"></script>
    <script src="/plugins/agencytheme/js/cbpAnimatedHeader.js"></script>
    <script src="/plugins/agencytheme/js/agency.js"></script>
    <script src="/plugins/agencytheme/js/jqBootstrapValidation.js"></script>
    <script src="/plugins/agencytheme/js/contact_me.js"></script>
{% endblock %}

{% block body %}
<!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                {# repeat: site #}{{ slots.logo|default('Logo')|raw }}
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                {# repeat: language #}{{ slots.menu|default('Menu')|raw }}
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>

    <!-- Header -->
    <header>
        <div class="container">
            <div class="intro-text">
                <div class="intro-lead-in">
                    {{ slots.intro|default('Intro')|raw }}
                </div>
                <div class="intro-heading">
                    {{ slots.intro_heading|default('Intro heading')|raw }}
                </div>
                {{ slots.more|default('Tell Me More')|raw }}
            </div>
        </div>
    </header>

    <!-- Services Section -->
    <section id="services">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">
                        {{ slots.services_title|default('Services')|raw }}
                    </h2>
                    <h3 class="section-subheading text-muted">
                        {{ slots.services_subtitle|default('Services subtitle')|raw }}
                    </h3>
                </div>
            </div>
                {{ slots.services|default('Services')|raw }}
        </div>
    </section>

    <!-- Portfolio Grid Section -->
    <section id="portfolio" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">
                        {{ slots.portfolio_title|default('Portfolio')|raw }}
                    </h2>
                    <h3 class="section-subheading text-muted">
                        {{ slots.portfolio_subtitle|default('Portfolio subtitle')|raw }}
                    </h3>
                </div>
            </div>
            {{ slots.portfolio|default('Portfolio')|raw }}
        </div>
    </section>
    <div id="porfolio-modals"></div>

    <!-- About Section -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">{{ slots.about_title|default('About')|raw }}</h2>
                    <h3 class="section-subheading text-muted">{{ slots.about_subtitle|default('About subtitle')|raw }}</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    {{ slots.about|default('About')|raw }}
                </div>
            </div>
        </div>
    </section>

    <!-- Team Section -->
    <section id="team" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">{{ slots.team_title|default('Our Amazing Team')|raw }}</h2>
                    <h3 class="section-subheading text-muted">{{ slots.team_subtitle|default('Team subtitle')|raw }}</h3>
                </div>
            </div>
            {{ slots.team|default('Team')|raw }}
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    {{ slots.team_footer|default('Team footer')|raw }}
                </div>
            </div>
        </div>
    </section>

    <!-- Clients Aside -->
    <aside class="clients">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6">
                    {{ slots.client_box_1|default('Client 1')|raw }}
                </div>
                <div class="col-md-3 col-sm-6">
                    {{ slots.client_box_2|default('Client 2')|raw }}
                </div>
                <div class="col-md-3 col-sm-6">
                    {{ slots.client_box_3|default('Client 3')|raw }}
                </div>
                <div class="col-md-3 col-sm-6">
                    {{ slots.client_box_4|default('Client 4')|raw }}
                </div>
            </div>
        </div>
    </aside>

    <!-- Contact Section -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">{{ slots.contact_title|default('Contact Us')|raw }}</h2>
                    <h3 class="section-subheading text-muted">{{ slots.contact_subtitle|default('Contact subtitle')|raw }}</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    {% include 'AgencyTheme/Resources/views/partials/_form.html.twig' %}
                </div>
            </div>
        </div>
    </section>

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <span class="copyright">{{ slots.copyright|default('Copyright')|raw }}</span>
                </div>
                <div class="col-md-4">
                    {{ slots.social_menu|default('Social menu')|raw }}
                    {#
                    <ul class="list-inline social-buttons">
                        <li><a href="#"><i class="fa fa-twitter"></i></a>
                        </li>
                        <li><a href="#"><i class="fa fa-facebook"></i></a>
                        </li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a>
                        </li>
                    </ul>#}
                </div>
                <div class="col-md-4">
                    {{ slots.quick_links|default('Quick links')|raw }}
                    {#
                    <ul class="list-inline quicklinks">
                        <li><a href="#">Privacy Policy</a>
                        </li>
                        <li><a href="#">Terms of Use</a>
                        </li>
                    </ul>#}
                </div>
            </div>
        </div>
    </footer>
{% endblock %}